package Main;
import java.sql.SQLException;
import java.text.ParseException;
import java.util.ArrayList;

import clases.Desarrolladores;

import Vistas.Form;

public class Main {
	
	/**
	 * @param args
	 * @throws SQLException 
	 * @throws ParseException 
	 */
	public static void main(String[] args) throws SQLException, ParseException {
		// TODO Auto-generated method stub
		Form form = new Form();
		
		ArrayList<Desarrolladores> empleados =form.getGestor().getDB().obtenerDesarrolladoresPorProyecto().get(1);
		for (int i = 0; i < empleados.size(); i++){
			System.out.println(empleados.get(i).getNombre() + " " + empleados.get(i).getDedicacionMensual());
		}
		
	}
	
}
